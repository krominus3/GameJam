    characterName: "\u0420\u0435\u0434\u0430\u043A\u0442\u043E\u0440"
    text: "\u0427\u0435\u0440\u0435\u0437 6 \u0434\u043D\u0435\u0439 \u0431\u0443\u0434\u0435\u0442
